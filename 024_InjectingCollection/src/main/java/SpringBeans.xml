<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="beanOgrenci" class="com.mimaraslan.Ogrenci">
		<property name="ogrenciList">
			<list>
				<value>Veli Çeri</value>
				<value>Serkan Erip</value>
				<value>Emer Piremerd</value>
			</list>
		</property>
		
		<property name="ogrenciSet">
			<set>
				<value>Veli Çeri</value>
				<value>Serkan Erip</value>
				<value>Emer Piremerd</value>
			</set>
		</property>
		
		<property name="ogrenciMap">
			<map>
				<entry key="1" value="Veli Çeri"/>
				<entry key="2" value="Serkan Erip"/>
				<entry key="3" value=">Emer Piremerd"/>
			</map>
		</property>
		
		<property name="ogrenciProp">
			<props>
				<prop key="Dürüst">Veli Çeri</prop>
				<prop key="Çay Sever">Serkan Edip</prop>
				<prop key="Saygılı">Emer Piremerd</prop>
			</props>
		</property>
