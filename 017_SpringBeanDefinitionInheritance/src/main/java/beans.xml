<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="selam" class="com.mimaraslan.Selam">
		<property name="mesaj1" value="Güzel gören, güzel düşünür." />
		<property name="mesaj2"
			value="Güzel düşünen, hayatından lezzet alır." />
	</bean>

	<bean id="yazar" class="com.mimaraslan.Yazar" parent="selam">
		<property name="mesaj1" value=" * * * "/>
		<property name="mesaj3" value="B.S.N."/>
	</bean>
</beans>
